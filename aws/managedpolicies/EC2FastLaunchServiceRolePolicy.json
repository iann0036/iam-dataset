  ],
  "arn": "arn:aws:iam::aws:policy/aws-service-role/EC2FastLaunchServiceRolePolicy",
  "createdate": "2025-05-14T17:07:07Z",
  "credentials_exposure": false,
  "data_access": false,
  "deprecated": false,
  "document": {
    "Statement": [
      {
        "Action": [
          "ec2:RunInstances"
        ],
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:subnet/*",
          "arn:aws:ec2:*:*:network-interface/*",
          "arn:aws:ec2:*::image/*",
          "arn:aws:ec2:*:*:key-pair/*",
          "arn:aws:ec2:*:*:security-group/*",
          "arn:aws:ec2:*:*:launch-template/*",
          "arn:aws:license-manager:*:*:license-configuration:*"
        ],
        "Sid": "AllowRunInstances"
      },
      {
        "Action": [
          "ec2:RunInstances"
        ],
        "Condition": {
          "StringEquals": {
            "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:volume/*",
          "arn:aws:ec2:*:*:instance/*"
        ],
        "Sid": "AllowRunInstancesOnFastLaunchCreatedResources"
      },
      {
        "Action": [
          "iam:PassRole"
        ],
        "Condition": {
          "StringEquals": {
            "iam:PassedToService": [
              "ec2.amazonaws.com",
              "ec2.amazonaws.com.cn"
            ]
          }
        },
        "Effect": "Allow",
        "Resource": "*",
        "Sid": "AllowPassRole"
      },
      {
        "Action": [
          "ec2:StopInstances",
          "ec2:TerminateInstances"
        ],
        "Condition": {
          "StringEquals": {
            "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:instance/*"
        ],
        "Sid": "AllowStopAndTerminateInstances"
      },
      {
        "Action": [
          "ec2:CreateSnapshot"
        ],
        "Condition": {
          "StringEquals": {
            "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:volume/*"
        ],
        "Sid": "AllowCreateSnapshot"
      },
      {
        "Action": [
          "ec2:CreateSnapshot"
        ],
        "Condition": {
          "ForAnyValue:StringEquals": {
            "aws:TagKeys": [
              "CreatedByLaunchTemplateName",
              "CreatedByLaunchTemplateId"
            ]
          },
          "StringEquals": {
            "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
          },
          "StringLike": {
            "aws:RequestTag/CreatedByLaunchTemplateVersion": "*"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:snapshot/*"
        ],
        "Sid": "AllowCreateTaggedSnapshot"
      },
      {
        "Action": [
          "ec2:CreateLaunchTemplate"
        ],
        "Condition": {
          "StringEquals": {
            "aws:RequestTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": "arn:aws:ec2:*:*:launch-template/*",
        "Sid": "AllowCreateLaunchTemplate"
      },
      {
        "Action": [
          "ec2:CreateTags"
        ],
        "Condition": {
          "StringEquals": {
            "ec2:CreateAction": [
              "CreateSnapshot",
              "RunInstances",
              "CreateLaunchTemplate"
            ]
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:volume/*",
          "arn:aws:ec2:*:*:instance/*",
          "arn:aws:ec2:*:*:snapshot/*",
          "arn:aws:ec2:*:*:launch-template/*"
        ],
        "Sid": "AllowCreateTags"
      },
      {
        "Action": [
          "ec2:DeleteSnapshot"
        ],
        "Condition": {
          "StringEquals": {
            "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:ec2:*:*:snapshot/*"
        ],
        "Sid": "AllowDeleteSnapshots"
      },
      {
        "Action": [
          "ec2:DescribeImages",
          "ec2:DescribeSnapshots",
          "ec2:DescribeSubnets",
          "ec2:DescribeInstanceAttribute",
          "ec2:DescribeInstanceStatus",
          "ec2:DescribeInstances",
          "ec2:DescribeInstanceTypeOfferings",
          "ec2:DescribeLaunchTemplateVersions",
          "ec2:DescribeLaunchTemplates",
          "cloudformation:DescribeStacks"
        ],
        "Effect": "Allow",
        "Resource": "*",
        "Sid": "AllowDescribeActions"
      },
      {
        "Action": [
          "cloudwatch:PutMetricData"
        ],
        "Condition": {
          "StringEquals": {
            "cloudwatch:namespace": "AWS/EC2"
          }
        },
        "Effect": "Allow",
        "Resource": "*",
        "Sid": "AllowPutMetricData"
      },
      {
        "Action": [
          "events:DeleteRule",
          "events:RemoveTargets",
          "events:PutRule",
          "events:PutTargets"
        ],
        "Condition": {
          "StringEquals": {
            "events:ManagedBy": "ec2fastlaunch.amazonaws.com"
          }
        },
        "Effect": "Allow",
        "Resource": [
          "arn:aws:events:*:*:rule/FastLaunch*"
        ],
        "Sid": "AllowEventsRuleMutations"
      },
      {
        "Action": [
          "events:ListTargetsByRule",
          "events:DescribeRule"
        ],
        "Effect": "Allow",
        "Resource": [
          "arn:aws:events:*:*:rule/FastLaunch*"
        ],
        "Sid": "AllowEventsRuleNonMutations"
      },
      {
        "Action": [
          "kms:ListRetirableGrants"
        ],
        "Effect": "Allow",
        "Resource": "*",
        "Sid": "AllowKMSActions"
      },
      {
        "Action": [
          "ec2:DeleteLaunchTemplate"
        ],
        "Condition": {
          "StringEquals": {
            "aws:ResourceTag/CreatedBy": "EC2 Fast Launch"
          }
        },
        "Effect": "Allow",
        "Resource": "*",
        "Sid": "AllowDeleteFastLaunchLaunchTemplates"
      }
    ],
    "Version": "2012-10-17"
  },
